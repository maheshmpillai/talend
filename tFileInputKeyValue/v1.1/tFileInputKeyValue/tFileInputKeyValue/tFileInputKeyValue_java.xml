<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="1.0"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Mahesh M.Pillai"
    RELEASE_DATE="20130104A"
    STARTABLE="true"
  >
    <SIGNATURE />
  </HEADER>

  <FAMILIES>
    <FAMILY>Input/Fields</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK" />
    <CONNECTOR CTYPE="COMPONENT_ERROR" />
    <CONNECTOR CTYPE="RUN_IF" />
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="FILENAME" FIELD="FILE" NUM_ROW="1" REQUIRED="true">
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/mahesh.csv"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="2">
    </PARAMETER>
    <PARAMETER NAME="FIELD_SEPARATOR" FIELD="TEXT" REQUIRED="true" NUM_ROW="3">
	<DEFAULT>","</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="ROW_SEPARATOR" FIELD="TEXT" REQUIRED="true" NUM_ROW="3">
	<DEFAULT>"\n"</DEFAULT>
    </PARAMETER>	
    <PARAMETER NAME="KEY_VALUE_SEPARATOR" FIELD="TEXT" REQUIRED="true" NUM_ROW="3">
	<DEFAULT>"\\|"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="KEY_NAMES" FIELD="TABLE" REQUIRED="true" NUM_ROW="4" NB_LINES="5">
      <ITEMS BASED_ON_SCHEMA="true">
        <ITEM NAME="KEY_NAME" />
        <ITEM NAME="TRIM" FIELD="CHECK" VALUE="false"/>
      </ITEMS>
    </PARAMETER>
    <PARAMETER NAME="HEADER" FIELD="TEXT" NUM_ROW="40" REPOSITORY_VALUE="HEADER">
		<DEFAULT>0</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="FOOTER" FIELD="TEXT" NUM_ROW="40" REPOSITORY_VALUE="FOOTER">
		<DEFAULT>0</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="LIMIT" FIELD="TEXT" NUM_ROW="40" REPOSITORY_VALUE="LIMIT">
		<DEFAULT></DEFAULT>
	</PARAMETER>
   	<PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="50">
		<DEFAULT>true</DEFAULT>
	</PARAMETER>   
  </PARAMETERS>

  <CODEGENERATION/>
<RETURNS>
      <RETURN AVAILABILITY="AFTER" NAME="NB_LINE" TYPE="id_Integer"/> 
 </RETURNS>
</COMPONENT>
